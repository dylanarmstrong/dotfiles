" options
" set toolbars=nomenu,noaddons,nonavigation,nobookmarks
set toolbars=nomenu,noaddons,nobookmarks
set runtimepath=~/.vimperator
set defsearch=d
set maxitems=9

" theme
colorscheme mine

" keybindings
nnoremap <silent> < :tabmove! -1<CR>
nnoremap <silent> > :tabmove! +1<CR>
nnoremap m :bmarks<space>
nnoremap v :tab viewsource<CR>
nnoremap ; :
nnoremap : ;
