LANG=en_US.UTF-8
LC_ALL=$LANG
LC_COLLATE=C

bindkey "^[[A" history-beginning-search-backward
bindkey "^[[B" history-beginning-search-forward

alias e='exit'
alias c='clear'
alias ps='ps aux'
alias vim='vim -X'
alias emerge='nocorrect emerge'

export VDPAU_NVIDIA_NO_OVERLAY=1
export MANPAGER=vimmanpager
