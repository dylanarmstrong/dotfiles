!RANDOM
Xcursor.theme: ComixCursors-Blue 

!XFT
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgba
Xft.hinting:    true
Xft.hintstyle:  hintslight
!Xft.hintstyle:  hintfull

!URXVT
URxvt.termName:		rxvt-256color
URxvt.scrollBar:	false
URxvt.buffered:		true
URxvt.loginShell: true

URxvt.font: xft:Ubuntu Mono:pixelsize=20
!URxvt.font: xft:Inconsolata:pixelsize=16
!URxvt.font: xft:Envy Code R:size=9
!URxvt.font: xft:Dejavu Sans Mono:pixelsize=16
!URxvt.font: xft:Anka/Coder-9
!URxvt.font: xft:terminus:pixelsize=18
!URxvt.font: -*-Fixed-medium-*-*-*-10-*-*-*-*-*-*-*

URxvt.clipboard.copycmd: xclip -i -selection clipboard
URxvt.clipboard.pastecmd: xclip -o -selection clipboard

URxvt.perl-ext-common:	default,url-select,keyboard-select,clipboard
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-s: perl:keyboard-select:search
URxvt.keysym.M-u:	perl:url-select:select_next

URxvt.urlLauncher: firefox
URxvt.url-select.button: 1
URxvt.underlineURLs: true
URxvt.transparent: true
URxvt.shading: 4
URxvt.geometry: 79x26

